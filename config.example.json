{
  "redis": {
    "host": "localhost",
    "port": 6380,
    "password": "",
    "db": 0,
    "timeout": 5000,
    "pool_size": 10,
    "tls": {
      "enabled": true,
      "cert_file": "/path/to/client.crt",
